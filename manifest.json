{
  "author": "igorlogius",
  "homepage_url": "https://github.com/igorlogius/group-same-site-tabs",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_area": "navbar"
  },
  "commands": {
    "group-selected": {
      "description": "Group Selected Sites"
    },

    "group-all": {
      "description": "Group All Sites"
    }
  },
  "icons": {
    "256": "icon.png"
  },
  "description": "group tabs from the same site",
  "manifest_version": 2,
  "name": "Group Same Site Tabs",
  "permissions": ["tabs", "menus", "tabGroups"],
  "version": "1.0.7"
}
